				<key>script</key>
				<string>function run(argv) {
	let [query] = argv;

	try {
		query = query.trim();

		let jsObject = parseJson(query);
		if (!jsObject) {
			eval(`jsObject = ${query}`);
		}
		return JSON.stringify(jsObject, null, 2);
	} catch (err) {
		return false;
	}
}

function parseJson(input) {
	try {
		return JSON.parse(input);
	} catch (err) {
		return;
	}
}</string>
